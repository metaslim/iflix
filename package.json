{
  "name": "iflix",
  "version": "1.0.0",
  "description": "iflix backend test coding experiment",
  "main": "index.js",
  "scripts": {
    "start_db": "mkdir -p data/db && mongod --dbpath data/db &",
    "stop_db": "rm -rf data/db/mongod.lock && ps -ef | grep mongod | grep -v grep | awk '{print \"kill -9 \"$2}' | sh",
    "reset_db": "rm -rf data/db/*",
    "seed_db": "node -e 'require(\"./db\").seed_data()'",
    "test": "mocha --exit",
    "start": "nodemon server.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.11"
  },
  "dependencies": {
    "async": "^2.6.0",
    "express": "^4.16.2",
    "mongoose": "^5.0.1"
  }
}
